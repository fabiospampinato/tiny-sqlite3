{
  "name": "tiny-sqlite3",
  "repository": "github:fabiospampinato/tiny-sqlite3",
  "description": "A tiny Node client for SQLite3, based on WASM",
  "version": "1.2.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "benchmark": "tsex benchmark",
    "benchmark:watch": "tsex benchmark --watch",
    "blob": "tsex task --name blob",
    "blob:watch": "tsex task --name blob --watch",
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "memory": "tsex task --name memory",
    "memory:watch": "tsex task --name memory --watch",
    "northwind": "bash tasks/northwind.sh",
    "test": "tsex test",
    "test:watch": "tsex test --watch",
    "prepublishOnly": "tsex prepare"
  },
  "keywords": [
    "sqlite3",
    "wasm",
    "node",
    "tiny"
  ],
  "dependencies": {
    "node-deno-sqlite": "^1.0.0",
    "stubborn-fs": "^1.2.5",
    "when-exit": "^2.1.1",
    "zeptoid": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.3",
    "fava": "^0.3.0",
    "tsex": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
