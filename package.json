{
  "name": "tiny-sqlite3",
  "repository": "github:fabiospampinato/tiny-sqlite3",
  "description": "A tiny cross-platform client for SQLite3, with precompiled binaries as the only third-party dependencies",
  "version": "1.2.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "benchmark": "tsex benchmark",
    "benchmark:watch": "tsex benchmark --watch",
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "northwind": "bash tasks/northwind.sh",
    "test": "tsex test",
    "test:watch": "tsex test --watch",
    "prepublishOnly": "tsex prepare"
  },
  "keywords": [
    "sqlite3",
    "native",
    "cross-platform",
    "tiny"
  ],
  "dependencies": {
    "function-once": "^3.0.0",
    "hex-encoding": "^2.0.2",
    "promise-make-naked": "^2.1.1",
    "stubborn-fs": "^1.2.5",
    "when-exit": "^2.1.1",
    "zeptoid": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "better-sqlite3": "^8.5.1",
    "fava": "^0.2.1",
    "sqlite-tag-spawned": "^0.6.0",
    "tsex": "^3.0.1",
    "typescript": "^5.1.6"
  }
}
